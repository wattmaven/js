{
	"extends": "@tsconfig/node20/tsconfig.json",
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"incremental": false,
		"isolatedModules": true,
		"noUncheckedIndexedAccess": true,
		"resolveJsonModule": true,
		"noEmit": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true
	}
}
